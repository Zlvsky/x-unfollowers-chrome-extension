{
  "name": "X/Twitter Unfollowers",
  "description": "Track who unfollowed you on X/Twitter",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "My first extension"
  },
  "icons": {
    "16": "vite.svg",
    "48": "vite.svg",
    "128": "vite.svg"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
